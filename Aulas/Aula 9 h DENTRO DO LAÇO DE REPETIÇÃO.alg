algoritmo "Estruturas de repetição até onde o usuário quiser alternando DENTRO DO LAÇO DE REPETIÇÃO"

var
contador, valor, soma: Inteiro
inicio
contador <- 1
soma <- 0
Enquanto (contador <= 5) faca
   Escreva ("Digite o", contador, "º valor: ")
   Leia (valor)
   soma <- soma + valor
   contador <- (contador + 1)
FimEnquanto
EscrevaL ("A soma de todos os valores foi ", S)
fimalgoritmo
