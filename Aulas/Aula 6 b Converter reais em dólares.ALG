Algoritmo "Conversor de real para dólar"

Var
   reais, dolares: real
Inicio
   Escreval ("Vamos converter reais em dólares.")
   Escreva ("Qual o valor em reais? R$")
   Leia (reais)
   Escreval ("***************************************")
   dolares <- reais / 5.33
   Escreval ("Quanto terei em dólares? US$",dolares)
   Escreval ("***************************************")
Fimalgoritmo